[application]

config/name="MikuVRMMotion"
run/main_scene="res://Scene.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[autoload]

StandardBones="*res://addons/vmd_motion/common/StandardBones.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/vsync/use_vsync=false

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PackedStringArray("res://addons/miku_motion_bake/plugin.cfg", "res://addons/Godot-MToon-Shader/plugin.cfg", "res://addons/vmd_motion/plugin.cfg", "res://addons/vrm/plugin.cfg")

[gui]

theme/use_hidpi=true

[importer_defaults]

